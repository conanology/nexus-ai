  "author": "Cryptology",
  "license": "MIT",
  "dependencies": {
    "@nexus-ai/core": "workspace:*",
    "googleapis": "^140.0.0",
    "image-size": "^2.0.2"
  },
  "devDependencies": {
    "@nexus-ai/config": "workspace:*",
    "@types/image-size": "^0.8.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
