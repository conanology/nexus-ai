{
  "name": "@nexus-ai/youtube",
  "version": "0.1.0",
  "description": "YouTube upload package with resumable uploads for NEXUS-AI pipeline",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
